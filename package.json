{
  "name": "typescript-express-starter",
  "version": "0.0.1",
  "description": "Typescript Express Starter",
  "main": "main.ts",
  "engines": {
    "node": "^10.0.0",
    "npm": "^6.0.0"
  },
  "scripts": {
    "start": "node ./src/www.js",
    "test": "NODE_ENV=test ts-node ./node_modules/jasmine/bin/jasmine",
    "test-reports": "NODE_ENV=test nyc --reporter=cobertura --reporter=html --reporter=text npm test",
    "compile": "node ./node_modules/typescript/bin/tsc -p ./src",
    "post-compile": "cp package*.json out",
    "watch": " node ./node_modules/typescript/bin/tsc -w -p ./src"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "typings",
      "**/*.interface.ts",
      "**/*.spec.ts",
      "src/app.ts",
      "src/www.ts",
      "src/models/*.ts",
      "src/routes/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-ts-mvc": "^0.0.3",
    "helmet": "^3.12.0",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jasmine": "^2.8.8",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.0.1",
    "jasmine": "^3.2.0",
    "node-mocks-http": "^1.7.0",
    "nodemon": "^1.18.1",
    "nyc": "^13.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.0.1"
  },
  "keywords": [
    "node",
    "jasmine",
    "typescript"
  ],
  "author": "Bill Cheng",
  "license": "ISC"
}
